{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-indent": [1, 2],
    "react/jsx-indent-props": [1, 2],
    "camelcase": "warn",
    "indent": ["warn", 2],
    "jsx-quotes": ["error", "prefer-double"],
    "max-len": "off",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "semi": ["error", "always"],
    "space-before-function-paren": "off"
  }
}
